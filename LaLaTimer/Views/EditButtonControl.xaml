             xmlns:local="clr-namespace:LaLaTimer.Views"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <Button Content="New" Command="{Binding EditCommand}">
        </Button>
    </Grid>
</UserControl>
